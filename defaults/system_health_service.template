{
  "id": "system_health_service",
  "name": "System Health Monitor",
  "description": "Monitors CPU and memory usage, logs metrics periodically",
  "version": "1.0.0",
  "author": "Sypnex OS Team",
  "check_interval": 30,
  "log_level": "DEBUG",
  "auto_start": true,
  "thresholds": {
    "cpu_warning": 80,
    "memory_warning": 85
  },
  "features": {
    "detailed_logging": true,
    "alert_on_thresholds": true
  }
}
